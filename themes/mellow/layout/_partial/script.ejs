<!-- main-js -->
<script type="text/javascript" src="//cdn.bootcss.com/jquery/2.0.0/jquery.min.js"></script>
<script type="text/javascript" src="<%- url_for(theme_js('/js/plugins/fastclick', cache)) %>"></script>
<% if (theme.waves) { %>
<script type="text/javascript" src="https://cdn.bootcss.com/node-waves/0.7.4/waves.min.js"></script>
<% } %>
<script type="text/javascript" src="<%- url_for(theme_js('/js/method', cache)) %>"></script>
<script type="text/javascript" src="<%- url_for(theme_js('/js/blog', cache)) %>"></script>

<!-- third-party -->
<% if (page.repo) { %>
    <%- partial("_third-party/github_repo") %>
<% } %>

<% if (theme.baidu_tongji) { %>
<%- partial('_third-party/baidu_tongji') %>
<% } %>

<% if (theme.local_search) { %>
<%- partial('_third-party/local_search') %>
<% } %>

<% if ( page.comments || typeof page.comments === "undefined" ) { %>
    <% if (theme.valine.enable) { %>
        <%- partial("_third-party/valine") %>
    <% } %>
    <% if (!theme.valine.enable && theme.gitment.enable) { %>
        <%- partial('_third-party/gitment') %>
    <% } %>
<% } %>

<% if (theme.leancloud_visitors.enable) { %>
<%- partial('_third-party/leancloud_visitors') %>
<% } %>

<% if (theme.mathjax.enable && ( page.mathjax || typeof page.mathjax === "undefined" ) ) { %>
    <%- partial("_third-party/mathjax") %>
<% } %>

<% if (theme.title_change) { %>
    <%- partial("_third-party/dynamic_title") %>
<% } %>

<% if (theme.daovoice.app_id) { %>
    <%- partial("_third-party/daovoice") %>
<% } %>